{
  "name": "telegram-replies-only-bot",
  "version": "1.0.0",
  "description": "Telegram bot to remove chat messages that are not replies",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write  '*.{json,js,md}' 'src/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js,md}' 'src/**/*.{js,ts}'",
    "prepare": "husky install"
  },
  "author": "author",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@cloudflare/wrangler": "^1.19.5",
    "@grammyjs/types": "^2.4.5",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "husky": "^7.0.4",
    "import-http": "^0.3.1",
    "prettier": "^2.5.1",
    "rollup": "^2.61.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "grammy": "^1.5.3"
  }
}
