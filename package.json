{
  "name": "telegram-replies-only-bot",
  "version": "1.0.0",
  "description": "Telegram bot to remove chat messages that are not replies",
  "module": "./dist/index.mjs",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write  '*.{json,js,md}' 'src/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js,md}' 'src/**/*.{js,ts}'",
    "prepare": "husky install"
  },
  "author": "author",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@cloudflare/wrangler": "^1.19.12",
    "@grammyjs/types": "^2.8.2",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "3.0.0",
    "husky": "^8.0.1",
    "import-http": "0.3.1",
    "prettier": "^2.7.1",
    "rollup": "^2.77.3",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "grammy": "1.10.1",
    "ip-cidr": "^3.0.10"
  }
}
